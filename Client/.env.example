# Copy to .env.development for local dev, and set these as Environment Variables in your hosting for production builds
# Frontend will call the backend using this base URL (no trailing slash)
VITE_BACKEND_URL=https://your-backend.onrender.com/api

# Optional: Cloudinary (these are client-side and will be embedded in the bundle)
VITE_CLOUD_NAME="<cloud_name>"
VITE_API_KEY="<api_key>"
VITE_SECRET_KEY="<secret_key>"
VITE_PRESET_NAME="<preset_name>"
